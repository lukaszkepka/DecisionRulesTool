<?xml version="1.0" encoding="utf-8" ?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <targets>
    <target xsi:type="Debugger"
            name="String"
            footer="Layout"
            layout="Layout"
            header="Layout" />
  </targets>
  <rules>
    <logger name="*" minlevel="Info" writeTo="LogFile" />
  </rules>
</nlog>